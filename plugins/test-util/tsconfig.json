{
  "compilerOptions": {
    "importHelpers": true,
    "module": "CommonJS",
    "outDir": "bin/",
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "removeComments": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "target": "es6",
    "sourceMap": true,
    "lib": [
      "es6",
    ],
    "types": [
      "jasmine"
    ]
  },
  "include": [
    "test-utils.ts"
  ]
}