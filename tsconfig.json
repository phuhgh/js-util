{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "bin/",
    "noImplicitReturns": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "target": "es5",
    "sourceMap": true,
    "lib": [
      "es5"
    ]
  }
}