{
  "name": "rc-js-util",
  "sideEffects": false,
  "version": "9.0.9-alpha.11",
  "license": "MIT",
  "scripts": {
    "test": "jasmine --config=jasmine.json",
    "copy-assets": " cp -r assets/release/* bin/",
    "build": "npm run build-ts && npm run build-cpp",
    "build-ts": "tsc -p tsconfig.json --incremental && npm run copy-assets",
    "build-cpp": "sh scripts/build-cpp.sh --bindings=bin/esm/cpp.module.js --cmake-dir=cpp --module-loader=mjs --build-mode=debug --cmake-flags=\"-DBUILD_JSUTIL_TEST=1\" && sh scripts/copy-test-modules.sh",
    "lib": "rimraf bin && npm run build-lib-cjs && npm run build-lib-esm && npm run copy-assets",
    "build-lib-cjs": "tsc -d -p tsconfig-cjs.lib.json",
    "build-lib-esm": "tsc -d -p tsconfig-esm.lib.json",
    "generate-docs": "rimraf docs tmp && api-extractor run && api-documenter markdown --input-folder tmp --output-folder docs",
    "lint": "eslint -c .eslintrc.cjs src/",
    "ci-build": "rimraf bin build && npm run build && npm run lint && npm run test && npm run lib && npm run generate-docs",
    "debug-test": "node --inspect-brk node_modules/jasmine/bin/jasmine.js --config=jasmine.json"
  },
  "dependencies": {
    "tslib": "^2.4.0",
    "rc-js-util-globals": "^7.0.1"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "~7.19.9",
    "@microsoft/api-extractor": "~7.20.1",
    "@types/jasmine": "^4.3.0",
    "jasmine": "^4.3.0",
    "jasmine-core": "^4.3.0",
    "rc-lint-rules": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "~8.22.0",
    "typescript": "~4.8.2",
    "rimraf": "^3.0.2"
  },
  "bin": {
    "rc-build-cpp": "scripts/build-cpp.sh"
  },
  "main": "bin/cjs/index.js",
  "typings": "bin/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./bin/esm/index.js",
      "require": "./bin/cjs/index.js"
    }
  },
  "keywords": [
    "Util",
    "stdlib",
    "Server",
    "Client",
    "Browser",
    "Emscripten",
    "WebAssembly"
  ],
  "repository": "https://github.com/phuhgh/js-util",
  "homepage": "https://phuhgh.github.io/js-util/rc-js-util.html"
}
