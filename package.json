
{
  "name": "rc-js-util",
  "version": "8.19.0",
  "license": "MIT",
  "scripts": {
    "test": "jasmine --config=jasmine.json",
    "build": "rimraf bin && npm run build-ts && npm run build-cpp",
    "build-ts": "tsc -d --outDir bin/cjs/ --module commonjs",
    "build-cpp": "sh build.sh",
    "lib": "rimraf bin && npm run build-lib-cjs && npm run build-lib-esm",
    "build-lib-cjs": "tsc -d -p tsconfig.lib.json --outDir bin/cjs/ --module commonjs",
    "build-lib-esm": "tsc -d -p tsconfig.lib.json --outDir bin/esm/ --module esnext",
    "generate-docs": "rimraf docs tmp && api-extractor run && api-documenter markdown --input-folder tmp --output-folder docs",
    "lint": "eslint -c node_modules/rc-lint-rules/.eslintrc.js src/",
    "ci-build": "npm run build && npm run lint && npm run test && npm run lib && npm run generate-docs",
    "debug-test": "node --inspect-brk node_modules/jasmine/bin/jasmine.js --config=jasmine.json"
  },
  "dependencies": {
    "tslib": "^2.4.0",
    "rc-js-util-globals": "^7.0.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "~7.19.9",
    "@microsoft/api-extractor": "~7.20.1",
    "@types/jasmine": "^4.3.0",
    "jasmine": "^4.3.0",
    "jasmine-core": "^4.3.0",
    "rc-lint-rules": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "~8.22.0",
    "typescript": "~4.5.5",
    "rimraf": "^3.0.2"
  },
  "bin": {
    "rc-extract-cpp-names": "scripts/extract-cpp-names.cjs"
  },
  "exports": {
    ".": {
      "import": "./bin/cjs/src/index.js",
      "require": "./bin/esm/src/index.js"
    }
  },
  "main": "bin/cjs/src/index.js",
  "typings": "bin/cjs/src/index.d.ts",
  "private": false,
  "keywords": [
    "Util",
    "stdlib",
    "Server",
    "Client",
    "Browser",
    "Emscripten",
    "WebAssembly"
  ],
  "repository": "https://github.com/phuhgh/js-util",
  "homepage": "https://phuhgh.github.io/js-util/rc-js-util.html"
}
