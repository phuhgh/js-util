{
  "name": "rc-js-util",
  "sideEffects": false,
  "version": "9.0.9-alpha.42",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf bin build",
    "test": "npm run test:js && npm run test:cpp",
    "test:js": "jasmine --config=jasmine.json",
    "test:cpp": "cd cpp/build/asan && ctest --no-tests=error -j 4 --output-on-failure --timeout 60",
    "copy-assets": " cp -r assets/release/* bin/",
    "build": "npm run build:ts && npm run copy-assets && npm run build:cpp",
    "build:ts": "tsc -p tsconfig.json --incremental",
    "build:cpp": "sh scripts/build-cpp.sh --bindings=bin/esm/cpp.module.js --cmake-dir=cpp --module-loader=mjs --cmake-preset=asan && sh scripts/copy-test-modules.sh --preset=asan",
    "lib": "npm run clean && npm run build-lib-cjs && npm run build-lib-esm && npm run copy-assets",
    "build-lib-cjs": "tsc -d -p tsconfig-cjs.lib.json",
    "build-lib-esm": "tsc -d -p tsconfig-esm.lib.json",
    "generate-docs": "rimraf docs tmp && api-extractor run && api-documenter markdown --input-folder tmp --output-folder docs",
    "lint": "eslint -c .eslintrc.cjs src/",
    "build:ci": "npm run clean && npm run build && npm run lint && npm run test && npm run lib && npm run generate-docs",
    "debug-test": "node --inspect-brk node_modules/jasmine/bin/jasmine.js --config=jasmine.json"
  },
  "dependencies": {
    "rc-js-util-globals": "^7.0.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "~7.24.2",
    "@microsoft/api-extractor": "~7.43.1",
    "@types/jasmine": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "~7.7.1",
    "@typescript-eslint/parser": "~7.7.1",
    "eslint": "~8.57.0",
    "eslint-plugin-import": "~2.29.1",
    "rc-lint-rules": "~10.0.1",
    "jasmine": "^5.1.0",
    "jasmine-browser-runner": "^2.1.0",
    "jasmine-core": "^5.1.0",
    "rimraf": "^4.4.1",
    "typescript": "~5.4.5"
  },
  "bin": {
    "rc-build-cpp": "scripts/build-cpp.sh"
  },
  "main": "bin/cjs/index.js",
  "typings": "bin/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./bin/esm/index.js",
      "require": "./bin/cjs/index.js"
    }
  },
  "keywords": [
    "Util",
    "stdlib",
    "Server",
    "Client",
    "Browser",
    "Emscripten",
    "WebAssembly"
  ],
  "repository": "https://github.com/phuhgh/js-util",
  "homepage": "https://phuhgh.github.io/js-util/rc-js-util.html"
}
